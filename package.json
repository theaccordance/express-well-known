{
  "name": "express-well-known",
  "version": "1.0.0",
  "description": "Well-Known Uniform Resource Identifiers",
  "main": "dist/index.js",
  "repository": "git@github.com:theaccordance/express-well-known.git",
  "author": "Joe Mainwaring <joseph.mainwaring@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "babel-jest": "^25.3.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "should": "^4.1.0",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:clear": "rm -rf dist/*",
    "nuke": "rm -rf node_modules/* dist/* coverage/*",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {}
}
